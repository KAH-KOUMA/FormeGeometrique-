package ds2kouma.ahamadou2ir;

/**
 *
 * @author KAH
 */
public class Carre implements FormeGeometrique{
    
    Point [] tab;
    public Carre()
    {
        tab = new Point[4];
    }          
    public Carre (Point a, Point b, Point c ,Point d)
    {
        this.tab[0] = a; this.tab[1] = b;
        this.tab[2] = c; this.tab[3] = d;
    }
    
    public float CalculPerimetre(){
        return 0;
    }
    
    public float CalculSurface () {
        return 0;
        
    }
    
    public Point [] getPoint (){
        return tab;
        
    }

    @Override
    public Point[] GetPoint() {
        throw new UnsupportedOperationException("Not supported yet."); //To change body of generated methods, choose Tools | Templates.
    }
}
